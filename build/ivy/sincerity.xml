<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info
		organisation="${maven.group}"
		module="${maven.artifact}"
		revision="${version}"
		status="release"
		default="true" />
	<configurations>
		<conf name="default" visibility="public" />
	</configurations>
	<dependencies>
		<dependency org="org.apache.commons" name="commons-vfs" rev="1.0">
			<exclude org="org.slf4j" />
		</dependency>
		<dependency org="org.apache.commons" name="commons-logging" rev="1.1.1" />
		<dependency org="org.jsoup" name="jsoup" rev="1.6.3" />
		<dependency org="com.threecrickets.scripturian" name="scripturian" rev="1.0.2" />
		<dependency org="org.mozilla" name="rhino" rev="1.7R4" />
		<dependency org="com.threecrickets.rhino" name="json-rhino" rev="1.0" />
		<!-- Note: When invoking Ant through the Maven Ant plugin, we cannot use the includeAntRuntime attributes in the javac task -->
		<dependency org="org.apache.ant" name="ant" rev="1.8.3" />
	</dependencies>
</ivy-module>
