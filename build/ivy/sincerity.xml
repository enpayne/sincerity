<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info
		organisation="${maven.group}"
		module="${maven.artifact}"
		revision="${version}"
		status="release"
		default="true" />
	<configurations>
		<conf name="default" visibility="public" />
	</configurations>
	<dependencies>
		<dependency org="org.apache.commons" name="commons-vfs" rev="1.0" />
		<dependency org="org.jsoup" name="jsoup" rev="1.6.0" />
		<dependency org="com.threecrickets.scripturian" name="scripturian" rev="1.0.0-R309" />
		<dependency org="org.mozilla" name="rhino" rev="1.7R3" />
		<dependency org="com.threecrickets.rhino" name="json-rhino" rev="1.0" />
	</dependencies>
</ivy-module>
